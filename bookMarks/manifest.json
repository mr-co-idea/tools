{
	"manifest_version": 2,
	"name": "bookMarks",
	"description": "To see the bookmarks used frequently",
	"version": "1.0",
	"permissions": ["storage", "bookmarks","activeTab","tabs"],
	"background": {
		"scripts": ["/src/actions/background.js", "src/actions/db.js"],
		"persistent": false
	},
	"browser_action": {
		"default_icon": "src/assets/images/icon.png",
		"default_popup": "src/views/popup.html"
	}
}